<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.gbmoro.diiin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logodiiin"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logodiiin"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="diiin.DindinApp">

        <activity android:name="diiin.ui.activity.MainActivity"
            android:configChanges="orientation|screenSize|screenLayout">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="diiin.ui.activity.InsertExpenseActivity"
            android:parentActivityName="diiin.ui.activity.MainActivity"
            android:label="@string/title_newexpense"/>

        <activity android:name="diiin.ui.activity.InsertSalaryActivity"
            android:parentActivityName="diiin.ui.activity.MainActivity"
            android:label="@string/title_newsalary"/>


    </application>
</manifest>
